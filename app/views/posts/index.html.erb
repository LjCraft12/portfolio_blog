<h1 id="page_title">Topics of my Interest</h1>

<div id="post_wrapper" class="skinny_wrapper">
  <% @posts.each do |post| %>
  <div class="post">
    <p class="date"><%= post.created_at.strftime('%e %b %Y %H:%M:%S%p') %></p>
    <h2><a href="#"><%= link_to post.title, post %></a></h2>
    <hr>
  </div>
      <% end %>
</div>

<%= will_paginate @posts %>
    <%=	link_to "Create new article", new_post_path %>
